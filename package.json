{
  "name": "tracing-tests",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "author": "Michael Arnaldi",
  "scripts": {
    "build": "ttsc -p tsconfig.build.json",
    "dev": "ts-node -C ttypescript -r tsconfig-paths/register src/index.ts",
    "start": "node -r source-map-support/register lib/index.js",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "ci": "yarn type-check && yarn test --ci",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@effect-ts/core": "^0.8.42",
    "@effect-ts/jest": "^0.2.42",
    "@effect-ts/monocle": "^0.7.43",
    "@effect-ts/morphic": "^0.8.59",
    "@effect-ts/tracing-plugin": "^0.0.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.14.11",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "fast-check": "^2.7.0",
    "fast-equals": "^2.0.0",
    "jest": "^26.6.3",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.2"
  }
}
