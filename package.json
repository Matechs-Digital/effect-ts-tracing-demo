{
  "name": "tracing-tests",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "author": "Michael Arnaldi",
  "scripts": {
    "build": "rimraf lib; ttsc --p tsconfig.build.json; yarn prettier:lib",
    "dev": "ts-node -C ttypescript src/index.ts",
    "start": "node lib/index.js",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "ci": "yarn type-check && yarn test --ci",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "prettier:lib": "prettier --write \"./lib/**/*.js\"",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@effect-ts/core": "^0.10.0",
    "@effect-ts/jest": "^0.4.0",
    "@effect-ts/monocle": "^0.9.0",
    "@effect-ts/morphic": "^0.10.0",
    "@effect-ts/node": "^0.2.4",
    "@effect-ts/system": "^0.8.0",
    "@effect-ts/tracing-plugin": "^0.2.0",
    "@effect-ts/tracing-utils": "^0.2.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "fast-check": "^2.7.0",
    "fast-equals": "^2.0.0",
    "jest": "^26.6.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-expose-internals": "^4.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "typescript-transform-paths": "^2.1.0"
  }
}
